<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">

	<title>@ViewData["Title"] - Patient Viewer</title>

	<environment include="Development">
		<link rel="stylesheet" type="text/css" href="~/lib/twitter-bootstrap/css/bootstrap.css" />
		<link rel="stylesheet" type="text/css" href="~/lib/font-awesome/css/font-awesome.css" />
	</environment>
	<environment exclude="Development">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.min.css"
			  asp-fallback-href="~/lib/twitter-bootstrap/css/bootstrap.min.css"
			  asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute"
			  integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous" />
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
			  asp-fallback-href="~/font-awesome/css/font-awesome.min.css"
			  asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute"
			  integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous" />
	</environment>
	<!-- Main CSS-->
	<link rel="stylesheet" type="text/css" href="~/css/main.css">
	<environment include="Development">
		<script src="~/lib/jquery/jquery.js"></script>
		<script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.js"></script>
	</environment>
	<environment exclude="Development">
		<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"
				asp-fallback-src="~/lib/jquery/jquery.min.js"
				asp-fallback-test="window.jQuery"
				integrity="sha384-vk5WoKIaW/vJyUAd9n/wmopsmNhiy+L2Z+SBxGYnUkunIxVxAv/UtMOhba/xskxh"
				crossorigin="anonymous"></script>
		<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/js/bootstrap.bundle.min.js"
				asp-fallback-src="~/lib/twitter-bootstrap/js/bootstrap.bundle.min.js"
				asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
				integrity="sha384-xrRywqdh3PHs8keKZN+8zzc5TX0GRTLCcmivcbNJWm2rs5C8PRhcEn3czEjhAO9o"
				crossorigin="anonymous"></script>
	</environment>
	<script type="text/javascript" src="~/js/site.js" asp-append-version="true"></script>
</head>
<body class="app sidebar-mini rtl">
	<!-- Header-->
	<partial name="Header" />
	<!-- Sidebar menu-->
	<partial name="SideBar" />
	<main class="app-content">
		@await Html.PartialAsync("AppTitle", ViewData["AppTitle"])
		@RenderBody()
	</main>
	@RenderSection("Scripts", required: false)
</body>
</html>