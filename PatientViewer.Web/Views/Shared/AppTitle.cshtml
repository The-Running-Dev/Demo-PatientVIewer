@model PatientViewer.Web.Models.AppTitle
@if (Model != null)
{
<div class="app-title">
	<div>
		<h1><i class="fa fa-th-list"></i> @Model.Title</h1>
	</div>
	<ul class="app-breadcrumb breadcrumb side">
		<li class="breadcrumb-item"><i class="fa fa-home fa-lg"></i></li>
		<li class="breadcrumb-item active"><a href="#">@Model.Title</a></li>
	</ul>
</div>
}